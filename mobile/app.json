{
  "expo": {
    "name": "NextLevel Coaching",
    "slug": "nextlevel-coaching",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1A1F20"
    },
    "assetBundlePatterns": ["**/*"],
    "scheme": "nextlevelcoaching",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nextlevel.coaching"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1A1F20"
      },
      "package": "com.nextlevel.coaching"
    },
    "web": {
      "bundler": "metro"
    }
  }
}
